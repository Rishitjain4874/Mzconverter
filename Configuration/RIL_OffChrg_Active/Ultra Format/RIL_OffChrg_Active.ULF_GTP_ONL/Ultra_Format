    ascii sgsnAddress                                                : terminated_by(",");
    ascii dataVolUpLink                                              : terminated_by(","),long(base10);
    ascii dataVolDownLink                                            : terminated_by(","),long(base10);
    ascii recSeqNum                                                  : terminated_by(","),int(base10);
